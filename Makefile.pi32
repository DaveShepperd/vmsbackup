# This is for a 32 bit PiOS version (Debian 32 bit on a Raspberry Pi). Only test on a Pi5.
# For now, vmsbackup has to be built to run in 32 bit mode.

HOST_MACH = 
HAVE_MTIO = 0
DELIM = '
PiOS32 = 1
LINUX = 1
MSYS2 = 0
MINGW = 0
EXTRA_DEFINES =
DBG = #-g -DDEBUG
OPT = -O -DNDEBUG
CC = gcc
ECHO = /bin/echo -e
RM = rm -f
RMDIR = rm -rf

include Makefile.common
